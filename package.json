{
  "name": "dudufisio-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc --project tsconfig.build.json && vite build",
    "start": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contract": "jest tests/contract",
    "test:integration": "jest tests/integration",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean-imports": "eslint . --ext ts,tsx --fix --rule 'no-console:error' --rule '@typescript-eslint/no-unused-vars:error' || true",
    "check-unused-exports": "ts-unused-exports tsconfig.json --showLineNumbers || true",
    "analyze-bundle": "vite build && vite build --mode analyze || true",
    "optimize-bundle": "node -e \"console.log('Use lazy routes, code splitting, and analyze-bundle');\"",
    "check-imports": "node scripts/check-imports.js",
    "pre-commit": "npm run build && npm run check-imports",
    "pre-commit-full": "npm run build && npm run check-imports && npm run lint && npm run type-check",
    "pre-deploy": "npm run pre-commit && npm audit --audit-level=high",
    "deploy": "bash scripts/deploy.sh",
    "setup-hooks": "husky install && chmod +x .husky/pre-commit",
    "postinstall": "husky install || true",
    "setup-vercel-env": "./scripts/setup-vercel-env.sh",
    "checkly:test": "checkly test",
    "checkly:deploy": "checkly deploy",
    "test:communication": "node scripts/test-communication-system.js",
    "test:integrations": "node scripts/test-vercel-integrations.js",
    "setup:vercel": "node scripts/setup-vercel-integrations.js"
  },
